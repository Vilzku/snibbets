{
  "name": "snibbets",
  "version": "1.0.0",
  "scripts": {
    "start": "yarn && concurrently \"yarn start:client\" \"yarn start:server\"",
    "dev": "yarn && concurrently \"yarn dev:client\" \"yarn dev:server\"",
    "start:client": "cd client && yarn && yarn start",
    "start:server": "yarn build:server && yarn start",
    "dev:client": "yarn start:client",
    "dev:server": "cd server && yarn && yarn dev",
    "build:client": "cd client && yarn && yarn build",
    "build:server": "cd server && yarn && yarn build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vilzku/snibbets.git"
  },
  "author": "Ville Puolitaival",
  "homepage": "https://github.com/Vilzku/snibbets#readme",
  "devDependencies": {
    "concurrently": "^6.4.0"
  }
}
